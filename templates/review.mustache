{{!
    @template local_registration/review

    Page for reviewing form data.
}}

<div class="box py-3 generalbox">
    {{#reviewdata}}
        <div class="form-group row fitem px-3">
            <div class="col-md-3 col-form-label d-flex pb-0 pr-md-0">
                {{key}}
            </div>
            <div class="col-md-9 form-inline align-items-start felement py-2">
                {{value}}
            </div>
        </div>
    {{/reviewdata}}

    <div class="form-group row fitem femptylabel">
        <div class="col-md-3 col-form-label d-flex pb-0 pr-md-0"> </div>
        <div class="col-md-9 form-inline align-items-start felement py-2">
            {{{editbutton}}}
            {{{submitbutton}}}
        </div>
    </div>
</div>
